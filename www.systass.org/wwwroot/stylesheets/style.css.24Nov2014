BODY{margin:5px 10px 0 10px;padding:10px;color:#fff;background-color:#ffc;font-family:'Trebuchet MS',Verdana,Tahoma,Arial,'Arial Black',Helvetica,'Century Gothic','Lucida Sans Unicode',Impact,sans-serif;font-size:small}div#content{position:relative;margin:auto;height:auto;min-height:850px;width:790px;padding:10px;background-color:gray;border-style:double;color:white}div#topBanner{font-family:"Trebuchet MS",sans-serif;font-size:.9em;position:relative;top:0;left:0;height:100px;padding:10px;margin:0;background-color:#369;color:white}div#topBanner img{float:right;height:90px}div#topBanner p{margin:0;padding:5px 5px;float:left;font-size:3em}div#leftCol{position:absolute;left:10px;height:auto;min-height:550px;float:left;width:110px;padding:5px 5px;margin:0;background-color:#963;color:white}div#rightCol{position:absolute;right:10px;min-height:550px;height:auto;_height:610px;width:175px;padding:2px 10px;background-color:#555;color:white}div#rightCol a{text-decoration:none;color:#ff0}div#rightCol a:hover{text-decoration:underline}div#centerCol{min-height:650px;height:auto;_height:650px;padding:0;margin:0 205px 50px 130px}div#centerCol table{border-spacing:0 20px}div#centerCol tr{vertical-align:top}div#centerCol tr.alternate{vertical-align:top;background-color:#999}div#centerCol td.quarter{width:25%}div#centerCol td.threequarter{width:75%}div#centerCol th{text-align:left;padding-right:5px}div#centerCol td{width:50%}div#centerCol a{text-decoration:none;color:#ff0}div#centerCol a:hover{text-decoration:underline}div#centerCol p.Reference{font-size:10.0pt;color:black;width:100%}div#centerCol p.Cite{font-size:10.0pt;color:black;width:90%}.title-top{font:bold 1.5em Verdana,sans-serif;border-bottom-style:solid;border-width:1px}div#links{position:relative;top:5px;left:0;width:110px;height:375px;font:small Verdana,sans-serif;z-index:100}div#links a{display:block;text-align:left;font:bold 1em sans-serif;padding:5px 10px;margin:0 0 1px;border-width:0;text-decoration:none;color:#FFC;background:#555}div#links a:hover{color:#411;background:#AAA}div#links a span{display:none}div#links a:hover span{display:block;position:absolute;top:320px;left:0;width:100px;padding:5px;margin:0;z-index:100;color:#FFC;background:#963;font:10px Verdana,sans-serif;text-align:center}div#search p{font-size:.9em}div#search a{text-decoration:none;color:#FFC;background:#963}div#search a:hover{text-decoration:underline;color:#FFC;background:#AAA}div#bottomBanner{display:block;vertical-align:middle;left:0;height:60px;padding:0 5px;margin:0;background-color:#369;color:white}div#bottomBanner p{margin:0 5px;padding:0 5px;float:left}div#bottomBanner img{float:right}div#bottomBanner a{text-decoration:none;color:#ff0}div#bottomBanner a:hover{text-decoration:underline}.title{font:1.5em "Comic Sans MS","TSCu_Comic",Verdana,sans-serif}.textsmall{font-size:.9em}p.topofpage{clear:both;padding:1em 0;background:url(/images/topofpage.gif) no-repeat center right;text-align:right;width:100%;font-size:90%}p.topofpage a{margin:0 25px 0 0}a[href $='.pdf']{padding-right:18px;background:transparent url(/images/icon_pdf.gif) no-repeat center right}a[href $='.doc']{padding-right:18px;background:transparent url(/images/icon_word.gif) no-repeat center right}a[href ^="mailto:"]{padding-right:20px;background:transparent url(/images/icon_mailto.gif) no-repeat center right}div#centerCol a[href^="http"]{background:url(/images/ext.gif) right center no-repeat;padding-right:12px}div#rightCol a[href^="http"]{background:url(/images/ext.gif) right center no-repeat;padding-right:12px}}div#centerCol a[href^="http://www.systass.org"]{background:transparent;padding-right:0}div#rightCol a[href^="http://www.systass.org"]{background:transparent;padding-right:0}div#leftCol a[href^="http://www.systass.org"]{background:transparent;padding-right:0}li{padding-bottom:25px}li.compact{padding-bottom:0}div#centerColForm{min-height:650px;height:auto;_height:650px;padding:0;margin:0 10px 50px 10px;background-color:white;color:black}div#GoogleMap{min-height:450px;height:auto;_height:450px;padding:0;margin:0 10px 50px 10px;background-color:white;color:black}div#GoogleMap a{text-decoration:none;color:#00f}div#GoogleMap a:hover{text-decoration:underline}
